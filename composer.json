{
    "name": "miw_upm/miw_results",
    "license": "MIT",
    "type": "project",
    "description": "MiW ORM Doctrine - Results",
    "homepage": "https://miw.etsisi.upm.es/",
    "keywords": ["doctrine-orm", "dotenv"],
    "authors": [
        {
            "name": "MiW UPM",
            "homepage": "https://miw.etsisi.upm.es/",
            "email": "miw.etsisi@upm.es",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "MiW\\Results\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiW\\Results\\Tests\\": "tests/"
        }
    },
    "require": {
		"php": ">=8.2",
        "ext-mysqli": "*",
        "ext-json": "*",
        "doctrine/orm": "^3.3",
        "vlucas/phpdotenv": "^5.6",
        "symfony/config": "^7.1",
        "symfony/cache": "^7.1",
        "symfony/routing": "^7.1",
        "symfony/yaml": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
